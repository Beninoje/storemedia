<ul class="row card-group ">
        <li class="card m-4 text-center col-lg-4">
            <h5>
                {{media.title}}
            </h5>
            <h6>
                {{media.type}}
            </h6>
            <p>{{media.releaseYear}}</p>
            <p>{{media.provider}}</p>
            <p>{{media.status}}</p>
            <p>{{media.rating}}</p>
            <p>{{media.username}}</p>
            <a href="/media/delete/{{media._id}}" class="btn btn-primary" onclick="return confirm('For real?!');">Delete</a>
            <a href="media/edit/{{media._id}}" class="btn btn-primary mt-2">Edit</a>
        </li>
</ul>