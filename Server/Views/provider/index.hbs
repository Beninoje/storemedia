<h1>Provider Library</h1>
<ul class="row card-group ">
    {{#each provider}}
        <li class="card m-4 text-center col-lg-4">
            <h5>
                {{this.name}}
            </h5>
            <a href="provider/edit/{{this._id}}" class="btn btn-primary mt-2">Edit</a>
        </li>
    {{/each}}
</ul>
<a href="/provider/create">Create New Provider</a>